<h1>Listing Attractions</h1>
<ul>
    <li>Name            Tickets         Description</li>
<% @attractions.each do |attraction|%>
    <li><%=attraction.name%>            <%= attraction.tickets%> <% if @user.admin%><%= link_to "Show #{attraction.name}", attraction_path(attraction)%> <%else%><%= link_to "Go on #{attraction.name}", attraction_path(attraction)%><%end%></li>
<%end%>

<% if @user.admin%>
    <%= link_to "New Attraction", new_attraction_path%>
<%end%>
</ul>
